{
  "name": "electron",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev ",
    "build": "electron-vite build",
    "electron": "wait-on tcp:3000 && cross-env IS_DEV=true electron .",
    "electron:dev": "concurrently -k  \"cross-env BROWSER=none npm run dev\" \"npm run electron\" ",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "electron:build.win": "npm run build && electron-builder --win --dir",
    "electron:build.linux": "npm run build && electron-builder --linux appImage",
    "electron:build.test": "npm run build && electron-builder --dir",
    "electron:build.exe": "npm run build && electron-builder --win"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@element-plus/icons-vue": "^2.0.10",
    "axios": "^1.3.5",
    "body-parser": "^1.20.2",
    "ccxt": "^3.0.61",
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "electron-updater": "^5.3.0",
    "element-plus": "^2.2.32",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.22",
    "qs": "^6.11.1",
    "sequelize": "^6.31.0",
    "sqlite3": "^5.1.6",
    "update-electron-app": "^2.0.1",
    "vue-router": "^4.1.6",
    "wait-on": "^7.0.1"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "16.18.11",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "electron": "^21.4.4",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.19",
    "eslint": "^8.31.0",
    "eslint-plugin-vue": "^9.8.0",
    "less": "^4.1.3",
    "prettier": "^2.8.2",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.22"
  }
}
